#include<stdio.h>
int main()
{
    int n,a[20],i=0, largest;
    printf("enter the suze off array:");
    scanf("%d",&n);
    for(int i=0;i<n;i++)
    {
        printf("%d value:",i+1);
        scanf("%d",&a[i]);
    }
    largest=a[0];
    while(i<n)
    {
        if(largest<a[i])
        {
            largest=a[i];
        }
        i++;
    }
    printf("The largest value is:%d", largest);
    return 0;
}
