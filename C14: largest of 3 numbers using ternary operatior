#include <stdio.h>

int main() {
    int a,b,c, largest;
    printf("enter a number A:");
    scanf("%d",&a);
    printf("enter a value of b:");
    scanf("%d",&b);
    printf("enter a value of C:");
    scanf("%d",&c);
    largest=(a>b&&a>c)?(a):((b>c&&b>a)?(b):(c));
    printf("%d\n", largest);
    return 0;
}
